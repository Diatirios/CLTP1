Number* Modulo::eval()
{
    Number *n1 = NULL, *n2 = NULL;
    n1 = getLeft()->eval();
    n2 = getRight()->eval();

    if (n1->isReal() || n2->isReal())
        return Factory::createRealNumber(n1->getReal() % n2->getReal());
    else
        return Factory::createIntegerNumber(n1->getInteger() % n2->getInteger());
}

